include ../../blocks/title/title

-categories = ['Эффект панорманой крыши', 'Антигравийная пленка', 'Оклейка кузова целиком', 'Винилография', 'Частичная оклейка кузова']

section.gallery
	.gallery__content
		+title(2, 'blue')(class='gallery__title')
			| Галерея
		ul.cat.cat--hidden
			each cat, i in categories
				if i === 0
					li.cat__item.cat__item--active
						span.cat__text(title=cat)=cat
				else
					li.cat__item
						span.cat__text(title=cat)=cat

		.carousel
			.carousel__stage

			.carousel__nav
				button.carousel__btn.carousel__btn--prev(type='button')
					svg.carousel__svg(
						version='1.1'
						id='Layer_1'
						xmlns='http://www.w3.org/2000/svg'
						xmlns:xlink='http://www.w3.org/1999/xlink'
						x='0px'
						y='0px'
						viewBox='0 0 15.2 32.5'
						style='enable-background:new 0 0 15.2 32.5;'
						xml:space='preserve')
						g
							image(
							style='overflow:visible;opacity:0.5;'
							width='22'
							height='40'
							xlink:href='data:image/png;base64,'
							transform='matrix(1 0 0 1 -2.252 -2.9991)')
							g
								g
									path(
									style='fill:#00A4B0;'
									d='M15.2,1.4c0-0.4-0.2-0.8-0.5-1.1c-0.6-0.5-1.5-0.4-2,0.2L0.3,15.3C0.1,15.6,0,15.9,0,16.3c0,0.3,0.1,0.7,0.3,0.9L12.7,32c0.5,0.6,1.4,0.7,2,0.2c0.3-0.3,0.5-0.7,0.5-1.1l0,0L15.2,1.4L15.2,1.4z')

				button.carousel__btn.carousel__btn--next(type='button')
					svg.carousel__svg(
						version='1.1'
						id='Layer_1'
						xmlns='http://www.w3.org/2000/svg'
						xmlns:xlink='http://www.w3.org/1999/xlink'
						x='0px'
						y='0px'
						viewBox='0 0 15.2 32.5'
						style='enable-background:new 0 0 15.2 32.5;'
						xml:space='preserve')
						g
							image(
								style='overflow:visible;opacity:0.5;'
								width='22'
								height='39'
								xlink:href='data:image/png;base64,'
								transform='matrix(1 0 0 1 -2 -1.9999)')
							g
								g
									defs
										rect(
											id='SVGID_1_'
											width='15.2'
											height='32.5')

									clipPath(id='SVGID_2_')
										use(
											xlink:href='#SVGID_1_'
											style='overflow:visible;')

								g
									defs
										rect(
											id='SVGID_3_'
											width='15.2'
											height='32.5')

									clipPath(id='SVGID_4_')
										use(
											xlink:href='#SVGID_3_'
											style='overflow:visible;')

									path(
										style='clip-path:url(#SVGID_4_);fill:#00A4B0;'
										d='M0,31.1c0,0.4,0.2,0.8,0.5,1.1c0.6,0.5,1.5,0.4,2-0.2l12.3-14.8c0.2-0.3,0.3-0.6,0.3-0.9c0-0.3-0.1-0.7-0.3-0.9L2.6,0.5C2-0.1,1.1-0.2,0.5,0.3C0.2,0.6,0,1,0,1.4l0,0L0,31.1L0,31.1z')

								g
									defs
										rect(
											id='SVGID_5_'
											width='15.2'
